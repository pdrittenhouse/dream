{
  "name": "Dream",
  "version": "5.0.3",
  "private": true,
  "description": "A WordPress Theme",
  "main": "docroot/index.php",
  "prefix": "../",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "cross-env NODE_ENV=development webpack serve --config ./webpack.dev.js",
    "serve:pl": "cd src/patternlab && php core/console -s",
    "plfilegen": "cross-env NODE_ENV=development webpack --config ./webpack.pl.js",
    "plgen": "cd src/patternlab && php core/console -g",
    "plregen": "cd src/patternlab && php core/console -g -p",
    "build:pl": "npm run plgen && npm run plfilegen",
    "build:dev": "cross-env NODE_ENV=development webpack --config ./webpack.dev.js",
    "build:env": "npm run plgen && npm run build:dev",
    "build:prod": "cross-env NODE_ENV=production webpack --config ./webpack.prod.js",
    "build": "webpack",
    "refresh:pl": "npm run plregen && npm run plfilegen",
    "refresh:env": "npm run refresh:pl && npm run build:dev",
    "watch:pl": "cd src/patternlab && php core/console -w -p",
    "watch:dev": "cross-env NODE_ENV=development webpack --config ./webpack.dev.js --watch",
    "watch": "concurrently -k \"npm run watch:pl\" \"npm run watch:dev\"",
    "start": "npm run build:pl && npm run build && npm run serve",
    "dev:watch": "concurrently -k \"npm run watch:pl\" \"npm run serve\"",
    "dev": "npm run build:env && npm run dev:watch",
    "stylelint-check": "stylelint-config-prettier-check",
    "new": "yo ./src/tools/patterngen/index.js"
  },
  "author": "Beaconfire RED",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.5",
    "@theme-tools/sass-export-data": "^1.1.1",
    "@wordpress/eslint-plugin": "^7.4.0",
    "@wordpress/prettier-config": "^0.4.0",
    "autoprefixer": "^10.0.2",
    "babel-loader": "^8.1.0",
    "chalk": "^4.1.0",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.1",
    "css-minimizer-webpack-plugin": "^1.1.5",
    "cssnano": "^4.1.10",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-webpack-plugin": "^2.2.1",
    "filemanager-webpack-plugin": "^3.0.0-alpha.7",
    "image-minimizer-webpack-plugin": "^1.0.0",
    "imagemin-gifsicle": "^7.0.0",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-svgo": "^8.0.0",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.3.0",
    "mqpacker": "^7.0.0",
    "node-sass": "^5.0.0",
    "postcss": "^8.1.9",
    "postcss-assets-webpack-plugin": "^4.0.0",
    "postcss-loader": "^4.1.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "2.1.2",
    "resolve-url-loader": "^3.1.2",
    "sass-loader": "^10.0.5",
    "stylelint": "^13.7.2",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-scss": "^3.18.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "svg-spritemap-webpack-plugin": "^3.7.1",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.3.0",
    "yeoman-generator": "^4.12.0",
    "yo": "^3.1.1",
    "yosay": "^2.0.2"
